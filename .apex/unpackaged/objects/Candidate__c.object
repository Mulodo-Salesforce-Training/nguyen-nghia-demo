    <validationRules>
        <fullName>Email_Suffix</fullName>
        <active>true</active>
        <description>Email of Candidate only available if it end with suffix @mulodo.com</description>
        <errorConditionFormula>AND(
	NOT(ISBLANK(Email__c)),
	NOT(REGEX( Email__c ,&quot;\\w([^@\\s,;]+)@(mulodo\\.com)&quot;))
)</errorConditionFormula>
        <errorMessage>Email of Candidate only available if it end with suffix @mulodo.com</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Min_Birthday</fullName>
        <active>true</active>
        <description>Candidates age should be greater than 16</description>
        <errorConditionFormula>NOT(ISBLANK(Birthday__c))&amp;&amp; ((YEAR(TODAY()) - YEAR(Birthday__c)) &lt; 16)</errorConditionFormula>
        <errorMessage>Candidates age should be greater than 16</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_Length</fullName>
        <active>true</active>
        <description>Phone Length should be between 10 and 11 number</description>
        <errorConditionFormula>LEN(SUBSTITUTE(Phone__c,&quot; &quot;,&quot;&quot;)) &lt;10 || LEN(SUBSTITUTE(Phone__c, &quot; &quot;,&quot;&quot;)) &gt; 11</errorConditionFormula>
        <errorMessage>Phone Length should be between 10 and 11 number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_OnlyDigit</fullName>
        <active>true</active>
        <description>Phone should contains  only digit</description>
        <errorConditionFormula>NOT(REGEX( Phone__c , &quot;^[0-9 ]+$&quot;))</errorConditionFormula>
        <errorMessage>Phone should contains only digit and whitespace</errorMessage>
    </validationRules>
